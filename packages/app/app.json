{
  "expo": {
    "name": "VSinder",
    "description": "Dating app for programmers",
    "slug": "vsinder",
    "scheme": "vsinder",
    "version": "1.2.14",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#002B36",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#002b36"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/notif-icon.png",
      "color": "#C14580",
      "androidMode": "collapse"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "bundleIdentifier": "com.benawad.vsinder",
      "buildNumber": "1.2.14"
    },
    "android": {
      "permissions": [],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "versionCode": 390010214,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#002b36"
      },
      "package": "com.benawad.vsinder"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    }
  }
}
